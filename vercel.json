{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://blog-website-phi-eight.vercel.app/blog"
    },
    {
      "source": "/blog/:slug*",
      "destination": "https://blog-website-phi-eight.vercel.app/blog/:slug*"
    }
  ],
  "git": {
    "submodules": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ]
}